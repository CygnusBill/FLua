<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>FLua.Cli</RootNamespace>
    <AssemblyName>flua</AssemblyName>
    <PublishAot>true</PublishAot>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\FLua.Interpreter\FLua.Interpreter.csproj" />
    <ProjectReference Include="..\FLua.Runtime\FLua.Runtime.csproj" />
    <ProjectReference Include="..\FLua.Compiler\FLua.Compiler.csproj" />
    <ProjectReference Include="..\FLua.Parser\FLua.Parser.fsproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" />
  </ItemGroup>

</Project>
